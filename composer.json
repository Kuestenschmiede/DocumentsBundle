{
  "name": "con4gis/documents",
  "type": "contao-bundle",
  "description": "Generating pdf documents directly or per stack",
  "keywords": ["con4gis", "document", "contao", "c4g", "con4gisdocuments", "con4gisdocumentsbundle", "c4gdocuments"],
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Kuestenschmiede",
      "homepage": "https://github.com/kuestenschmiede"
    }
  ],
  "require": {
    "con4gis/core": "^2.0",
    "dompdf/dompdf": "~0.8"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "con4gis\\DocumentsBundle\\": ""
    },
    "classmap": [
      "Resources/contao/"
    ],
    "exclude-from-classmap": [
      "Resources/contao/config/",
      "Resources/contao/dca/",
      "Resources/contao/languages/",
      "Resources/contao/templates/"
    ]
  },
  "support": {
    "issues": "https://github.com/Kuestenschmiede/DocumentBundle/issues"
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "contao-manager-plugin": "con4gis\\DocumentsBundle\\ContaoManager\\Plugin"
  }
}